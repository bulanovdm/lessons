<xsd:schema xmlns:cf="http://www.raiffeisen.ru/types/CustomField/v1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.raiffeisen.ru/types/CustomField/v1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.1">
	<xsd:complexType name="ct_CustomField">
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string"/>
			<xsd:element name="value" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ct_CustomFieldList">
		<xsd:sequence>
			<xsd:element name="customField" type="cf:ct_CustomField" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
